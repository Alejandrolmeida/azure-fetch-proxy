# Example Environment Configuration
# Copy this file to .env and customize for your setup

# Security Configuration
API_KEY=your-secure-api-key-here-change-this-in-production

# Spanish Geolocation Configuration
SPANISH_REGIONS=Madrid,Barcelona,Valencia,Sevilla,Bilbao
SPANISH_ISPS=telefonica.net,orange.es,vodafone.es,euskaltel.es,jazztel.es
DEFAULT_SPANISH_REGION=Madrid

# Rate Limiting Configuration  
MAX_REQUESTS_PER_MINUTE=30
MAX_REQUESTS_PER_HOUR=1000

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
REQUEST_TIMEOUT=30

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Security Headers
ENABLE_SPANISH_HEADERS=true
CUSTOM_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36

# Custom Domain Configuration
CUSTOM_DOMAIN=proxy.azurebrains.com

# Azure specific (optional)
# AZURE_INSIGHTS_INSTRUMENTATION_KEY=your-app-insights-key
